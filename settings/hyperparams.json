{
    "results_root": "./results_full",
    "clean_results_dir": false,
    "clean_weights_dir": false,

    "cuda": true,
    "gpu": 0,

    "reconstruction_loss": "combined",
    "metrics": [],

    "dataset": "custom",
    "data_dir": "/shared/sets/datasets/3D_points/shapenet_points",
    "csv_files_dir": "/home/z1142375/dev/HyperCloud/docs/test_data",
    
    "classes": [],

    "comment" : ["airplane", "bag", "basket","bathtub", "bed",  "bench",
        "bicycle", "birdhouse", "bookshelf", "bottle", "bowl", "bus",
        "cabinet",  "can",  "camera",  "cap",  "car",  "chair",
        "clock", "dishwasher", "monitor", "table", "telephone", "tin_can",
        "tower", "train", "keyboard", "earphone", "faucet", "file",
        "guitar", "helmet", "jar", "knife", "lamp", "laptop",
        "speaker", "mailbox", "microphone", "microwave", "motorcycle", "mug",
        "piano", "pillow", "pistol", "pot", "printer", "remote_control",
        "rifle", "rocket",  "skateboard", "sofa", "stove",  "watercraft",
        "washer", "boat", "cellphone"],
    
    "shuffle": true,
    "transforms": ["rotate"],
    "num_workers": 20,
    "n_points": 15000,

    "max_epochs": 4000,
    "batch_size": 32,
    "gradient_penalty_coef": 10,
    "reconstruction_coef": 0.05,
    "z_size": 2048,
    "pointnet": false,
    "feature_regularization_coef": 0.001,
    "wasserstein": true,

    "p": 0.2,
    "z_beta_a": 0.01,
    "z_beta_b": 0.01,

    "normal_mu": 0.0,
    "normal_std": 0.2,

    "seed": 2018,
    "save_frequency": 100,
    "epsilon": 0.001,

    "target_network_input": {
        "constant": false,
        "points": {
            "disable_after" : 1999
        },

        "colors": {
            "enable_after": 2000
        },

        "normalization": {
            "enable": false,
            "type": "progressive",
            "epoch": 1
        }

    },

    "model": {
        "D": {
            "dropout": 0.5,
            "use_bias": true,
            "relu_slope": 0.2
        },
        "P_HN": {
            "use_bias": true,
            "relu_slope": 0.2
        },
        "CP_HN": {
            "use_bias": true,
            "relu_slope": 0.2
        },
        "P_E": {
            "use_bias": true,
            "relu_slope": 0.2
        },
        "CP_E": {
            "use_bias": true,
            "relu_slope": 0.2
        },
        "TN": {
            "use_bias": true,
            "relu_slope": 0.2,
            "freeze_layers_learning": false,
            "layer_out_channels": [32, 64, 128, 64]
        }
    },
    "optimizer": {
        "D": {
            "type": "Adam",
            "hyperparams": {
                "lr": 0.00005,
                "weight_decay": 0,
                "betas": [0.9, 0.999],
                "amsgrad": false
            }
        },
        "E_HN": {
            "type": "Adam",
            "hyperparams": {
                "lr": 0.00005,
                "weight_decay": 0,
                "betas": [0.9, 0.999],
                "amsgrad": false
            }
        }
    }
}
